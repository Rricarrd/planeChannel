FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

 
#include "$FOAM_CASE/constant/fieldData/0.000/U"



boundaryField
{
    // Spatial or temporal case
    
    
        
            "(inlet|outlet)"
            {
                type            cyclic;
            }
            "(left|right)"
            {
                type        cyclic;
            }
        

    
    

    // Static or oscillating BC/domain
    

        
            top
            {
                type            noSlip;
            }

            bottom
            {
                #include "$FOAM_CASE/system/lowerWallTranspiration"
            }

        

    

    

    
    
}


