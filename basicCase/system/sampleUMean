// -*- C++ -*-

type                sets;
libs                ("libsampling.so");
interpolationScheme cellPoint;
setFormat           raw;

fields
(
    columnAverage(UMean)
);

sets
{
    l1 // 1, 5, 10, ... delta
    {
        type        uniform;
        axis        distance;
        start       (40.0 0 1.4959965021);
        end         (40.0 2 1.4959965021);
        nPoints     200;
    }    
}


