FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

{% if schemes=="linear" or schemes=="cubic"%}

  ddtSchemes
  {
      default         backward;
  }

  gradSchemes
  {
      default         Gauss {{schemes}};
  }

  divSchemes
  {
      default         none;
      div(phi,U)      Gauss {{schemes}};
      div((nuEff*dev2(T(grad(U))))) Gauss {{schemes}};
  }

  laplacianSchemes
  {
      default         Gauss {{schemes}} orthogonal;
  }

  interpolationSchemes
  {
      default         {{schemes}};
  }

  snGradSchemes
  {
      default         orthogonal;
  }

{% else %}

  ddtSchemes
  {
      default         backward;
  }

  gradSchemes
  {
      default         leastSquares;
  }

  divSchemes
  {
      default         none;
      div(phi,U)      Gauss cubic;
      div((nuEff*dev2(T(grad(U))))) Gauss cubic;
  }

  laplacianSchemes
  {
      default         Gauss cubic orthogonal;
  }

  interpolationSchemes
  {
      default         cubic;
  }

  snGradSchemes
  {
      default         orthogonal;
  }

{% endif %}


