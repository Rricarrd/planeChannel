FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      cellDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];
internalField   uniform (0 0 0);

boundaryField
{
  bottom
  {
      type        groovyBC;
      value       uniform (0 0 0);
      variables "A=0;k=2*pi/5.60998688141;Uwx=1;";
      valueExpression "vector(0,A*sin(k*pos().x-Uwx*time()),0)";
  }
  
  top
  {
      type        fixedValue;
      value       uniform (0 0 0);
  }
  
  
  
  
      "(inlet|outlet)"
      {
          type            cyclicAMI;
      }
      "(left|right)"
      {
          type        cyclicAMI;
      }
  
  
}
// ************************************************************************* //